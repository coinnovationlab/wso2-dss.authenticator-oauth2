<?xml version="1.0" encoding="UTF-8"?>
<serviceGroup>
    <service name="OAUTH2SSOAuthenticationService" scope="transportsession">
        <transports>
            <transport>https</transport>
        </transports>
        <parameter name="ServiceClass" locked="false">
            org.wso2.carbon.identity.authenticator.oauth2.sso.OAUTH2SSOAuthenticator
        </parameter>
        <parameter name="DoAuthentication" locked="true">false</parameter>
        <excludeOperations>
            <operation>isHandle</operation>
            <operation>isAuthenticated</operation>
            <operation>getPriority</operation>
            <operation>getAuthenticatorName</operation>
            <operation>isDisabled</operation>
        </excludeOperations>
    </service>
    <parameter name="hiddenService" locked="true">true</parameter>
</serviceGroup>
